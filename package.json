{
  "name": "nova-whitespace",
  "version": "1.3.0",
  "description": "Provides an editor command for trimming trailing whitespace characters from the focused editor document.",
  "main": "index.js",
  "scripts": {
    "build": "npx rollup -c",
    "build:dist": "npm run build && rm -r *.novaextension/node_modules",
    "test:lint": "npx eslint \"Source/**/*.js\"",
    "watch": "onchange -i \"Source/**/*.js\" -- npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsmecham/nova-whitespace.git"
  },
  "keywords": [],
  "author": "Justin Mecham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsmecham/nova-whitespace/issues"
  },
  "homepage": "https://github.com/jsmecham/nova-whitespace#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-nova": "^1.0.0-beta.3",
    "onchange": "^6.1.0",
    "rollup": "^1.31.0"
  },
  "dependencies": {
    "nova-npm-executable": "^0.1.0"
  },
  "private": true
}
